use Drupal\Core\Cache\cache;

/* implements hook entity type insert*/
function wizzlern_pegi_node_insert(Drupal\Core\Entity\EntityInterface $entity){
  if ($entity->bundle() == 'game'){
    Cache::invalidateTags(['wizzlern_pegi_new_game']);
  }
}
